                <div class="price" id="pricing"></div>
                <div class="title" id="due-date">Fecha de corte</div>
                <div class="price" id="due-date-date">08/09/23</div>
              </div>
            </div>
          </div>
        </div>
        <div class="access" id="access" style="display: none">
          <h1>Lenguaje</h1>
          <div class="access-content" style="display: flex;justify-content: center;align-items: center;margin-top: 5%;">
            <button class="flag" style="background-color: transparent;border: none;" >
              <a href="/perfil"><img src="https://upload.wikimedia.org/wikipedia/commons/thumb/9/9a/Flag_of_Spain.svg/2560px-Flag_of_Spain.svg.png" alt="Español" style="width: 200px;"></a>
            </button>
            <button class="flag" style="background-color: transparent;border: none;">
              <a href="/profile"><img src="data:image/png;base64," style="width: 250px;" alt="English"></a>
            </button>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
<script
  src="https://code.jquery.com/jquery-3.7.0.js"
  integrity="sha256-JlqSTELeR4TLqP0OG9dxM7yDPqX1ox/HfgiSLBj8+kM="
  crossorigin="anonymous"
></script>
<script>
  $("#billingbtn").on("click", function () {
    $("#info-content").fadeOut();
    $("#billing").fadeIn();
    $("#access").fadeOut();
    if (document.getElementById("billing-plan").innerHTML == "Begginner") {
      document.getElementById("pricing").innerHTML = "Free";
    }
    if (document.getElementById("billing-plan").innerHTML == "Standard") {
      document.getElementById("pricing").innerHTML = "$1.99";
    }
    if (document.getElementById("billing-plan").innerHTML == "Elite") {
      document.getElementById("pricing").innerHTML = "$4.99";
    }
  });

  $("#personalinfo").on("click", function () {
    $("#info-content").fadeIn();
    $("#billing").fadeOut();
    $("#access").fadeOut();
  });

  $("#acc").on("click", function () {
    $("#info-content").fadeOut();
    $("#billing").fadeOut();
    $("#access").fadeIn();
  });
</script>
